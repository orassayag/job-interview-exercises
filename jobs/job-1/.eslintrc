{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "quotes": [
            "error",
            "single",
            {
                "allowTemplateLiterals": true
            }
        ],
        "no-shadow": [
            "error",
            {
                "builtinGlobals": false,
                "hoist": "functions",
                "allow": []
            }
        ],
        "default-case": 0,
        "comma-dangle": 1,
        "semi": 1,
        "no-useless-constructor": 0,
        "no-useless-escape": 0,
        "no-console": 0,
        "no-debugger": 0,
        "no-empty": 0,
        "linebreak-style": 0,
        "import/first": 0,
        "import/imports-first": 0,
        "no-async-promise-executor": 0,
        "smart-tabs": 0,
        "no-tabs": 0,
        "no-new-wrappers": 0,
        "no-mixed-operators": 0,
        "require-atomic-updates": 0,
        "no-unused-vars": 1,
        "disable-next-line": 0,
        "no-case-declarations": 0,
        "require-await": 1,
        "no-mixed-spaces-and-tabs": 0,
        "no-unused-expressions": 1
    }
}